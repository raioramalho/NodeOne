generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model Example {
  id    Int    @id @default(autoincrement())
  email String @unique

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("example")
}
